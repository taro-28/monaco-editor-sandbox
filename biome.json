{
  "$schema": "https://biomejs.dev/schemas/1.8.3/schema.json",
  "organizeImports": {
    "enabled": true
  },
  "json": {
    "linter": {
      "enabled": false
    }
  },
  "linter": {
    "rules": {
      "all": true,
      "style": {
        "noNonNullAssertion": "off",
        "noDefaultExport": "off"
      }
    }
  },
  "formatter": {
    "enabled": true,
    "indentStyle": "space",
    "indentWidth": 2
  }
}
